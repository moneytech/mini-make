set (PACKAGE_VERSION @MINI_MAKE_VERSION@)

if("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL 0)

  # compatible with any version 0.x
  set(PACKAGE_VERSION_COMPATIBLE FALSE)

  if("${PACKAGE_FIND_VERSION_MINOR}" EQUAL 2)
    # exact match for version 0.2
    set(PACKAGE_VERSION_EXACT TRUE)
  endif("${PACKAGE_FIND_VERSION_MINOR}" EQUAL 2)

else("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL 0)
  set (PACKAGE_VERSION_COMPATIBLE FALSE)
  set (PACKAGE_VERSION_UNSUITABLE TRUE)
endif("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL 0)
